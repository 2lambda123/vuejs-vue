{
  "root": true,
  "plugins": ["flowtype", "prettier"],
  "extends": ["plugin:vue-libs/recommended", "plugin:flowtype/recommended", "prettier"],
  "rules": {
    "prettier/prettier": [
      2,
      {
        "bracketSpacing": false,
        "semi": false,
        "printWidth": 80,
        "singleQuote": true,
        "trailingComma": "all"
      }
    ]
  },
  "globals": {
    "__WEEX__": true
  }
}
